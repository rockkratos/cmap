<!-- Start Notifications -->

<div class="dn">
	
	<div id="warning-wrapper">
		<div class="hint-box hint-warning">
			<a href="javascript:void(0);" class="close-hint-msg"><img src="${rc.contextPath}/imgs/admin/icons/cross-grey-small.gif" /></a>
			<div>
				<img src="${rc.contextPath}/imgs/admin/icons/exclamation.gif" class="vm" />
				<span class="vm">提示信息</span>
			</div>
		</div>
	</div>
	
	<div id="msg-wrapper">
		<div class="hint-box hint-msg">
			<a href="javascript:void(0);" class="close-hint-msg"><img src="${rc.contextPath}/imgs/admin/icons/cross-grey-small.gif" /></a>
			<div>
		    	<img src="${rc.contextPath}/imgs/admin/icons/information.gif" class="vm" />
				<span class="vm">提示信息</span>
			</div>
		</div>
	</div>
	
	<div id="success-wrapper">
		<div class="hint-box hint-success">
			<a href="javascript:void(0);" class="close-hint-msg"><img src="${rc.contextPath}/imgs/admin/icons/cross-grey-small.gif" /></a>
			<div>
				<img src="${rc.contextPath}/imgs/admin/icons/tick-circle.gif" class="vm" />
				<span class="vm">提示信息</span>
			</div>
		</div>
	</div>
	
	<div id="error-wrapper">
		<div class="hint-box hint-error">
			<a href="javascript:void(0);" class="close-hint-msg"><img src="${rc.contextPath}/imgs/admin/icons/cross-grey-small.gif" /></a>
			<div>
		    	<img src="${rc.contextPath}/imgs/admin/icons/cross-circle.gif" class="vm" />
				<span class="vm">提示信息</span>
			</div>
		</div>
	</div>
	
</div>

<!-- End Notifications -->
